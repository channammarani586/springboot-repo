pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Starting the build process...'
                echo 'Build completed successfully.'
            }
        }

        stage('Test') {
            steps {
                echo 'Starting the test process...'
                echo 'Tests completed successfully.'
            }
        }
    }

    post {
        always {
            echo 'Pipeline execution finished.'
        }
    }
}
